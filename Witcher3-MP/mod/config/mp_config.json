{
  "mod_info": {
    "name": "Witcher3-MP Multiplayer Server",
    "version": "1.0.0",
    "author": "Witcher3-MP Team",
    "description": "Multiplayer server mod for The Witcher 3: Wild Hunt Next-Gen",
    "compatibility": "4.0+",
    "priority": 1000
  },
  "server_config": {
    "default_port": 60000,
    "max_players": 8,
    "server_name": "Witcher3-MP Server",
    "auto_start": false,
    "password_protected": false,
    "password": "",
    "whitelist_enabled": false,
    "whitelist": []
  },
  "network_config": {
    "tick_rate": 60,
    "compression_enabled": true,
    "encryption_enabled": false,
    "timeout_seconds": 30,
    "max_packet_size": 1024,
    "buffer_size": 8192
  },
  "game_config": {
    "sync_players": true,
    "sync_npcs": true,
    "sync_items": true,
    "sync_quests": false,
    "sync_weather": true,
    "sync_time": true,
    "sync_combat": true,
    "sync_magic": true,
    "sync_inventory": false
  },
  "optimization_config": {
    "low_latency_mode": false,
    "dynamic_optimization": true,
    "prediction_enabled": true,
    "interpolation_enabled": true,
    "batching_enabled": true,
    "priority_system": true
  },
  "logging_config": {
    "log_level": "INFO",
    "log_to_file": true,
    "log_to_console": true,
    "log_file_path": "logs/witcher3_mp.log",
    "max_log_size_mb": 10,
    "log_rotation": true
  },
  "ui_config": {
    "show_server_status": true,
    "show_player_list": true,
    "show_chat": true,
    "ui_scale": 1.0,
    "ui_position": "top_right"
  },
  "compatibility": {
    "witcher3_version": "4.0+",
    "redkit_required": true,
    "mod_manager": "vortex",
    "script_merger": true,
    "asset_override": true
  },
  "features": {
    "multiplayer_enabled": true,
    "coop_quests": false,
    "pvp_enabled": false,
    "shared_world": true,
    "voice_chat": false,
    "text_chat": true,
    "friend_system": false,
    "guild_system": false
  }
}
